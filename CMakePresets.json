{
  "version": 3,
  "configurePresets": [
    {
      "name": "build",
      "displayName": "Build (Torch from TORCH_HOME)",
      "generator": "Unix Makefiles",
      "binaryDir": "${sourceDir}/build",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_PREFIX_PATH": "$env{TORCH_HOME}/share/cmake/Torch",
        "CMAKE_BUILD_RPATH": "$env{TORCH_HOME}/lib",
        "CMAKE_INSTALL_RPATH": "$env{TORCH_HOME}/lib",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
      }
    },
    {
      "name": "debug",
      "displayName": "Debug (Torch from TORCH_HOME)",
      "generator": "Unix Makefiles",
      "binaryDir": "${sourceDir}/debug",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "CMAKE_PREFIX_PATH": "$env{TORCH_HOME}/share/cmake/Torch",
        "CMAKE_BUILD_RPATH": "$env{TORCH_HOME}/lib",
        "CMAKE_INSTALL_RPATH": "$env{TORCH_HOME}/lib",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
      }
    },
    {
      "name": "release",
      "displayName": "Release (Torch from TORCH_HOME)",
      "generator": "Unix Makefiles",
      "binaryDir": "${sourceDir}/release",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "CMAKE_PREFIX_PATH": "$env{TORCH_HOME}/share/cmake/Torch",
        "CMAKE_BUILD_RPATH": "$env{TORCH_HOME}/lib",
        "CMAKE_INSTALL_RPATH": "$env{TORCH_HOME}/lib",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
      }
    }
  ],
  "buildPresets": [
    {
      "name": "build",
      "configurePreset": "build"
    },
    {
      "name": "debug",
      "configurePreset": "debug"
    },
    {
      "name": "release",
      "configurePreset": "release"
    }
  ]
}